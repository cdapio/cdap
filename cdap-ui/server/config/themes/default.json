{
  "spec-version": "1.0",
  "styles": {
    "brand-primary-color": "#3b78e7",
    "navbar-color": "#333333",
    "font-family": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif",
    "page-name-color": "#bbbbbb",
    "plugin-reference-heading-bg-color": "#999999"
  },
  "content": {
    "product-name": "CDAP",
    "product-description": "CDAP is an open source framework that simplifies data application development, data integration, and data management.",
    "product-logo-navbar": {
      "type": "link",
      "arguments": {
         "url": "/cdap_assets/img/company_logo-20-all.png"
      }
    },
    "product-logo-about": {
      "type": "link",
      "arguments": {
         "url": "/cdap_assets/img/CDAP_darkgray.png"
      }
    },
    "favicon-path": "cdap_assets/img/favicon.png",
    "footer-text": "Licensed under the Apache License, Version 2.0",
    "footer-link": "https://www.apache.org/licenses/LICENSE-2.0"
  },
  "features": {
    "about-product": true,
    "dashboard": true,
    "reports": true,
    "data-prep": true,
    "pipelines": true,
    "analytics": true,
    "rules-engine": true,
    "metadata": true,
    "hub": true,
    "ingest-data": true,
    "add-namespace": true,
    "schedules": true,
    "triggers": true,
    "lineage": true,
    "realtime-pipeline": true,
    "native-profile": true,
    "application-upload": true,
    "system-services-instance": true,
    "system-metrics": true,
    "namespace-mapping": true,
    "namespace-security": true,
    "create-profile": true,
    "reload-system-artifacts": true
  }
}
