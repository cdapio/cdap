  },
  "jest": {
    "testEnvironment": "jsdom",
    "testPathDirs": [
      "./app/cdap/"
    ],
    "modulePaths": [
      "./app/cdap/"
    ],
    "moduleNameMapper": {
      "^components": "<rootDir>/components",
      "^services": "<rootDir>/services",
      "^api": "<rootDir>/api",
      "^mocks": "<rootDir>/__mocks__",
      "^.+\\.less$": "<rootDir>/__mocks__/styleMocks.js"
    }
