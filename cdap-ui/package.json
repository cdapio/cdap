{
  "name": "cdap-ui",
  "version": "4.0.0-SNAPSHOT",
  "description": "Front-end for CDAP",
  "scripts": {
    "start": "node ./server.js",
    "prebuild": "npm install && ./node_modules/bower/bin/bower update --config.interactive=false",
    "build": "node ./node_modules/gulp/bin/gulp.js build",
    "cdap-dev-build": "webpack --dev-tools --watch -d",
    "cdap-prod-build": "NODE_ENV=production webpack -p",
    "distribute": "node ./node_modules/gulp/bin/gulp.js distribute",
    "test": "node ./node_modules/karma/bin/karma start test/karma-conf.js",
    "test-single-run": "node ./node_modules/karma/bin/karma start test/karma-conf.js --no-auto-watch --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caskdata/cdap/cdap-ui"
  },
