{
    "artifact": {
        "name": "cdap-data-pipeline",
        "version": "6.1.3-SNAPSHOT",
        "scope": "SYSTEM"
    },
    "description": "Data Pipeline Application",
    "name": "missing-artifact",
    "config": {
