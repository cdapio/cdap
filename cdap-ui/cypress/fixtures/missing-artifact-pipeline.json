                    "properties": {
                        "field": "*",
                        "precondition": "false",
                        "threshold": "1",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"body\",\"type\":[\"bytes\",\"null\"]}]}",
                        "workspaceId": "b6d9b1a7-bf7d-4ed2-8b12-2a1130742a0c",
