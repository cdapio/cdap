        "connections": [
            {
                "from": "File",
                "to": "Wrangler"
            },
            {
                "from": "Wrangler",
                "to": "PDFExtractor"
            },
            {
                "from": "PDFExtractor",
                "to": "Trash"
            }
        ],
        "comments": [],
        "postActions": [],
        "properties": {},
        "processTimingEnabled": true,
        "stageLoggingEnabled": false,
        "stages": [
            {
                "name": "File",
