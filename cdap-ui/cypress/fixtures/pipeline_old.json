{
  "artifact": {
    "name": "cdap-data-pipeline",
    "version": "6.2.0-SNAPSHOT",
    "scope": "SYSTEM",
    "label": "Data Pipeline - Batch"
  },
  "description": "",
  "name": "AmazonPIpeline",
  "config": {
    "resources": {
      "memoryMB": 1024,
      "virtualCores": 1
    },
    "driverResources": {
      "memoryMB": 1024,
      "virtualCores": 1
    },
    "connections": [
      {
        "from": "Amazon S3",
        "to": "Amazon S32"
      }
    ],
    "comments": [],
    "postActions": [],
    "properties": {},
    "processTimingEnabled": true,
    "stageLoggingEnabled": true,
    "stages": [
      {
        "name": "Amazon S3",
        "plugin": {
          "name": "S3",
          "type": "batchsource",
          "label": "Amazon S3",
          "artifact": {
            "name": "amazon-s3-plugins1",
            "version": "1.9.2",
            "scope": "USER"
          },
          "properties": {
            "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"offset\",\"type\":\"long\"},{\"name\":\"body\",\"type\":\"string\"}]}",
            "format": "text",
            "authenticationMethod": "Access Credentials",
            "filenameOnly": "false",
            "recursive": "false",
            "ignoreNonExistingFolders": "false",
            "referenceName": "sdf",
            "path": "sdf"
          }
        },
        "outputSchema": [
          {
            "name": "etlSchemaBody",
            "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"offset\",\"type\":\"long\"},{\"name\":\"body\",\"type\":\"string\"}]}"
          }
        ]
      },
      {
        "name": "Amazon S32",
        "plugin": {
          "name": "S3",
          "type": "batchsink",
          "label": "Amazon S32",
          "artifact": {
            "name": "amazon-s3-plugins1",
            "version": "1.9.2",
            "scope": "USER"
          },
          "properties": {
            "suffix": "yyyy-MM-dd-HH-mm",
            "format": "json",
            "authenticationMethod": "Access Credentials",
            "enableEncryption": "true",
            "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"offset\",\"type\":\"long\"},{\"name\":\"body\",\"type\":\"string\"}]}",
            "referenceName": "sdf",
            "path": "sdf"
          }
        },
        "outputSchema": [
          {
            "name": "etlSchemaBody",
            "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"offset\",\"type\":\"long\"},{\"name\":\"body\",\"type\":\"string\"}]}"
          }
        ],
        "inputSchema": [
          {
            "name": "Amazon S3",
            "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"offset\",\"type\":\"long\"},{\"name\":\"body\",\"type\":\"string\"}]}"
          }
        ]
      }
    ],
    "schedule": "0 * * * *",
    "engine": "mapreduce",
    "numOfRecordsPreview": 100,
    "maxConcurrentRuns": 1
  }
}
